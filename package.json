{
  "name": "@byjohann/ui",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@9.15.3",
  "description": "A collection of Vue.js components for my projects",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/components.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/components/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "csstype": "^3",
    "vue": "^3"
  },
  "dependencies": {
    "@byjohann/utils": "^0.7.2",
    "@nuxt/kit": "^3.15.1",
    "@nuxt/schema": "^3.15.1"
  },
  "optionalDependencies": {
    "medium-zoom": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@iconify-json/ph": "^1.2.2",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@types/node": "^22.10.5",
    "@unocss/eslint-config": "^65.4.0",
    "@unocss/reset": "^65.4.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^12.4.0",
    "bumpp": "^9.10.0",
    "csstype": "^3.1.3",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "unocss": "^65.4.0",
    "vite": "^6.0.7",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "pnpm": {
    "overrides": {
      "mkdist": "^1.6.0"
    }
  }
}
