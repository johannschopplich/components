{
  "name": "@byjohann/ui",
  "type": "module",
  "version": "0.5.5",
  "packageManager": "pnpm@8.10.2",
  "description": "A collection of Vue.js components for my projects",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/components.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/components/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.mjs"
    },
    "./mapbox": {
      "types": "./dist/mapbox/index.d.ts",
      "import": "./dist/mapbox/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "components.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "vue": "^3"
  },
  "optionalDependencies": {
    "@types/mapbox-gl": "^2.7.17",
    "mapbox-gl": "^2.15.0",
    "medium-zoom": "^1.0.8"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.1"
  },
  "devDependencies": {
    "@byjohann/eslint-config": "^0.3.0",
    "@iconify-json/ph": "^1.1.6",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/node": "^20.8.10",
    "@unocss/eslint-config": "^0.57.2",
    "@unocss/reset": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/core": "^10.5.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.57.2",
    "vite": "^4.5.0",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  }
}
