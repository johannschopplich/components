{
  "name": "@byjohann/ui",
  "type": "module",
  "version": "0.6.5",
  "packageManager": "pnpm@8.15.5",
  "description": "A collection of Vue.js components for my projects",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/components.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/components/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.mjs"
    },
    "./mapbox": {
      "types": "./dist/mapbox/index.d.ts",
      "import": "./dist/mapbox/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "vue": "^3"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.1"
  },
  "optionalDependencies": {
    "@types/mapbox-gl": "^3.1.0",
    "mapbox-gl": "^3.2.0",
    "medium-zoom": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@iconify-json/ph": "^1.1.11",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@types/node": "^20.11.30",
    "@unocss/eslint-config": "^0.58.6",
    "@unocss/reset": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10.9.0",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "unbuild": "^3.0.0-rc.1",
    "unocss": "^0.58.6",
    "vite": "^5.1.6",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
