{
  "name": "@byjohann/ui",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@9.8.0",
  "description": "A collection of Vue.js components for my projects",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/components.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/components/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.mjs"
    },
    "./mapbox": {
      "types": "./dist/mapbox/index.d.ts",
      "import": "./dist/mapbox/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "vue": "^3"
  },
  "dependencies": {
    "@byjohann/utils": "^0.6.0",
    "@nuxt/kit": "^3.13.0",
    "@nuxt/schema": "^3.13.0"
  },
  "optionalDependencies": {
    "@types/geojson": "^7946.0.14",
    "mapbox-gl": "^3.6.0",
    "medium-zoom": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@iconify-json/ph": "^1.1.14",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@types/node": "^20.16.1",
    "@unocss/eslint-config": "^0.62.2",
    "@unocss/reset": "^0.62.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vueuse/core": "^11.0.1",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7",
    "unocss": "^0.62.2",
    "vite": "^5.4.2",
    "vue": "^3.4.38",
    "vue-tsc": "^2.0.29"
  }
}
